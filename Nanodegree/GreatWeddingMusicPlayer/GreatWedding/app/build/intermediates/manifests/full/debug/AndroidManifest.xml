<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rajczyk.michalina.great_wedding"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="rajczyk.michalina.great_wedding.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="rajczyk.michalina.great_wedding.DinnerMusicActivity"
            android:label="@string/dinner_music_label" />
        <activity
            android:name="rajczyk.michalina.great_wedding.RomanticMusicActivity"
            android:label="@string/romantic_music_label" />
        <activity
            android:name="rajczyk.michalina.great_wedding.HitsActivity"
            android:label="@string/hits_label" />
        <activity
            android:name="rajczyk.michalina.great_wedding.WildPartyActivity"
            android:label="@string/wild_party_music_label" />
        <activity
            android:name="rajczyk.michalina.great_wedding.YourPlaylistActivity"
            android:label="@string/your_playlist_label" />
    </application>

</manifest>